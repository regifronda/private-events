<div class="created events">
  <h1>Created Events</h1>
  <% @user_events.each do |user_event| %>
    <strong><%= user_event.name %><br></strong>
    <p><%= user_event.date %></p>
    <p><%= user_event.location %></p>
    <p><%= user_event.description %></p>
  <% end %>
  <h1>Attending Events:</h1>
  <% @user.attended_events.order(date: :asc).where("date > ?", Date.today).each do |event| %>
    <p><%= event.name %></p>
  <% end %>
</div>